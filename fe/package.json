{
	"aliases": {
		"$lib": "src/lib"
	},
	"dependencies": {
		"@svizzle/ui": "^0.9.1",
		"@svizzle/utils": "^0.19.0",
		"eslint-import-resolver-custom-alias": "^1.3.0",
		"js-yaml": "^4.1.0",
		"just-compare": "^2.3.0",
		"lamb": "^0.61.0",
		"rison2": "^0.2.3",
		"svelte-json-tree": "^1.0.0",
		"xstate": "^4.35.2"
	},
	"description": "Dapsboard Front-end",
	"devDependencies": {
		"@sveltejs/adapter-auto": "^1.0.2",
		"@sveltejs/adapter-netlify": "^1.0.5",
		"@sveltejs/adapter-static": "^1.0.5",
		"@sveltejs/kit": "^1.2.9",
		"@svizzle/dev": "^0.6.1",
		"@svizzle/file": "^0.14.1",
		"@xstate/inspect": "^0.7.0",
		"chmod": "^0.2.1",
		"clipboardy": "^3.0.0",
		"eslint-import-resolver-custom-alias": "^1.3.0",
		"eslint-plugin-svelte": "^2.15.0",
		"esm-module-alias": "^2.0.2",
		"mkdirp": "^2.1.3",
		"mocha": "^10.2.0",
		"npm-run-all": "^4.1.5",
		"rimraf": "^4.1.2",
		"rison-esm": "^0.0.1",
		"svelte": "^3.55.1",
		"svelte-check": "^3.0.3",
		"typescript": "^4.9.4",
		"vite": "^4.0.4"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"husky": {
		"hooks": {
			"pre-push": "npm run lint"
		}
	},
	"name": "dapsboard_fe",
	"private": true,
	"scripts": {
		"build": "vite build",
		"chmod": "chmod u+x ./src/bin/*",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"copy_builder_form": "node --loader esm-module-alias/loader src/bin/machines/builder_copy_form.js",
		"copy_builder_route": "node --loader esm-module-alias/loader src/bin/machines/builder_copy_route.js",
		"copy_explore_route": "node --loader esm-module-alias/loader src/bin/machines/explore_copy_route.js",
		"deploy": "npm run makedata && npm run build",
		"dev": "vite dev",
		"devinspect": "export INSPECT=true && vite dev",
		"lint": "eslint .",
		"makedata": "npm run mkdirdata && npm run chmod && node --loader esm-module-alias/loader ./src/bin/make_data.js",
		"makedata2": "npm run mkdirdata && npm run chmod && node ./src/bin/make_data.js",
		"mkdirdata": "mkdirp ./src/lib/app/data",
		"prepare": "npm run makedata && svelte-kit sync",
		"preview": "vite preview",
		"redev": "npm run rmkv && npm run prepare && npm run dev",
		"rmkit": "rimraf .svelte-kit",
		"rmkv": "npm run rmkit && npm run rmvite",
		"rmvite": "rimraf node_modules/.vite",
		"unittests": "mocha --spec src/**/*.spec.js"
	},
	"type": "module",
	"version": "0.0.1"
}
