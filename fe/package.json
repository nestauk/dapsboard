{
	"dependencies": {
		"@svizzle/utils": "^0.12.0",
		"js-yaml": "^4.1.0",
		"compression": "^1.7.4",
		"just-compare": "^2.3.0",
		"lamb": "^0.61.0",
		"polka": "^0.5.2",
		"sirv": "^2.0.2",
		"svelte-json-tree": "^1.0.0",
		"xstate": "^4.14.0"
	},
	"description": "Dapsboard Front-end",
	"devDependencies": {
		"@rollup/plugin-babel": "^6.0.3",
		"@rollup/plugin-commonjs": "^24.0.0",
		"@rollup/plugin-json": "^6.0.0",
		"@rollup/plugin-node-resolve": "^15.0.1",
		"@rollup/plugin-replace": "^5.0.2",
		"@rollup/plugin-terser": "^0.3.0",
		"@rollup/plugin-yaml": "^4.0.1",
		"@svizzle/dev": "^0.4.1",
		"@svizzle/file": "^0.9.0",
		"@xstate/inspect": "^0.2.0",
		"clipboardy": "^3.0.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"esm": "^3.2.25",
		"husky": "^4.3.0",
		"mkdirp": "^2.1.3",
		"mocha": "^7.1.1",
		"npm-run-all": "^4.1.5",
		"rimraf": "^4.1.1",
		"rison-esm": "0.0.1",
		"rollup-plugin-cleanup": "^3.2.1",
		"rollup-plugin-svelte": "^7.0.0",
		"rollup": "^3.10.0",
		"sapper": "^0.29.3",
		"svelte": "^3.29.7"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"husky": {
		"hooks": {
			"pre-push": "npm run lint"
		}
	},
	"name": "dapsboard_fe",
	"scripts": {
		"build": "sapper build --legacy",
		"chmod": "chmod u+x ./src/bin/*",
		"copy_builder_form": "node -r esm src/bin/machines/builder_copy_form.js",
		"copy_builder_route": "node -r esm src/bin/machines/builder_copy_route.js",
		"copy_explore_route": "node -r esm src/bin/machines/explore_copy_route.js",
		"deploy": "npm run makedata && npm run export",
		"dev": "sapper dev",
		"devinspect": "export INSPECT=true && sapper dev",
		"export": "export SAPPER_EXPORT=true && sapper export --legacy",
		"legacyinstall": "npm install --legacy-peer-deps",
		"lint": "eslint rollup.config.js 'src/**/*.{js,svelte}'",
		"makedata": "npm run mkdirdata && npm run chmod && node -r esm ./src/bin/make_data.js",
		"mkdirdata": "mkdirp ./src/node_modules/app/data",
		"rmsappers": "rimraf __sapper__ src/node_modules/@sapper",
		"start": "node __sapper__/build",
		"test": "run-p --race dev cy:run",
		"unittests": "mocha -r esm --spec src/**/*.spec.js"
	}
}
