{
  "dependencies": {
    "@svizzle/utils": "^0.7.1",
    "compression": "^1.7.4",
    "lamb": "^0.58.0",
    "polka": "^0.5.2",
    "sirv": "^0.4.2",
    "svelte-json-tree": "^0.1.0"
  },
  "description": "Nesta DAPS dashboard",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/runtime": "^7.9.2",
    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^2.3.2",
    "@rollup/plugin-yaml": "^2.1.0",
    "@svizzle/dev": "^0.3.0",
    "@svizzle/file": "^0.7.0",
    "clipboardy": "^2.3.0",
    "eslint": "^7.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "esm": "^3.2.25",
    "husky": "^4.2.5",
    "js-yaml": "^3.13.1",
    "mkdirp": "^1.0.4",
    "mocha": "^7.1.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.7.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-svelte": "^5.2.1",
    "rollup-plugin-terser": "^5.3.0",
    "sapper": "^0.27.12",
    "svelte": "^3.20.1",
    "typedoc": "^0.17.8",
    "typescript": "3.9.5",
    "xstate": "^4.11.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  },
  "license": "MIT",
  "name": "dapsboard",
  "repository": "github:nestauk/dapsboard",
  "scripts": {
    "build": "sapper build --legacy",
    "chmod": "chmod u+x ./src/bin/*",
    "deploy": "npm run makedata && npm run makejsondocs && npm run export",
    "dev": "sapper dev",
    "export": "export SAPPER_EXPORT=true && sapper export --legacy",
    "lint": "eslint rollup.config.js 'src/**/*.{js,svelte}'",
    "makedata": "npm run mkdirdata && npm run mkdirdsl && npm run chmod && node -r esm ./src/bin/make_data.js",
    "makejsondocs": "typedoc --json src/node_modules/app/data/typedoc_out.json static/dsl/datasets.ts && node -r esm ./src/bin/make_docs.js",
    "mkdirdata": "mkdirp ./src/node_modules/app/data",
    "mkdirdsl": "mkdirp ./static/dsl",
    "start": "node __sapper__/build",
    "test": "run-p --race dev cy:run",
    "unittest": "mocha -r esm --spec src/**/*.spec.js",
    "copy_builder": "node -r esm src/bin/builder_machine.js"
  },
  "version": "0.0.6"
}
