{
  "dependencies": {
    "@xstate/inspect": "^0.1.1",
    "compression": "^1.7.4",
    "lamb": "^0.59.2",
    "polka": "^0.5.2",
    "sirv": "^0.4.2",
    "svelte-json-tree": "^0.1.0",
    "xstate": "^4.13.0"
  },
  "description": "Nesta DAPS dashboard",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-yaml": "^2.1.0",
    "@svizzle/dev": "^0.4.1",
    "@svizzle/file": "^0.9.0",
    "@svizzle/utils": "^0.11.1",
    "babel-eslint": "^10.1.0",
    "clipboardy": "^2.3.0",
    "eslint": "^7.10.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "esm": "^3.2.25",
    "husky": "^4.3.0",
    "js-yaml": "^3.14.0",
    "mkdirp": "^1.0.4",
    "mocha": "^7.1.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rison-esm": "0.0.1",
    "rollup": "^2.28.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-svelte": "^5.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.27.16",
    "svelte": "^3.29.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  },
  "license": "MIT",
  "name": "dapsboard",
  "repository": "github:nestauk/dapsboard",
  "scripts": {
    "build": "sapper build --legacy",
    "builder_copy_form": "node -r esm src/bin/machines/builder_copy_form.js",
    "builder_copy_route": "node -r esm src/bin/machines/builder_copy_route.js",
    "chmod": "chmod u+x ./src/bin/*",
    "deploy": "npm run makeall && npm run export",
    "dev": "sapper dev",
    "devinspect": "export INSPECT=true && sapper dev",
    "explore_copy_route": "node -r esm src/bin/machines/explore_copy_route.js",
    "export": "export SAPPER_EXPORT=true && sapper export --legacy",
    "lint": "eslint rollup.config.js 'src/**/*.{js,svelte}'",
    "makeall": "npm run makedata",
    "makedata": "npm run mkdirdata && npm run mkdirdsl && npm run chmod && node -r esm ./src/bin/make_data.js",
    "mkdirdata": "mkdirp ./src/node_modules/app/data",
    "mkdirdsl": "mkdirp ./static/dsl",
    "rmsappers": "rimraf __sapper__ src/node_modules/@sapper",
    "start": "node __sapper__/build",
    "test": "run-p --race dev cy:run",
    "unittest": "mocha -r esm --spec src/**/*.spec.js"
  },
  "version": "0.0.6"
}
